[
    { "key": "cmd+p", "command": "workbench.action.showCommands" },
    { "key": "cmd+o", "command": "workbench.action.quickOpen"    },
    { "key": "cmd+j", "command": "workbench.action.togglePanel"  },

    { "key": "cmd+s", "command": "editor.action.triggerSuggest"  },
    { "key": "k", "command": "selectPrevSuggestion", "when": "suggestWidgetVisible" },
    { "key": "j", "command": "selectNextSuggestion", "when": "suggestWidgetVisible" },

    { "key": "cmd+a", "command": "extension.vim_ctrl+[" },

    { "key": "cmd+n", "command": "workbench.action.files.newUntitledFile", "when": "editorTextFocus" },
    { "key": "cmd+q", "command": "workbench.action.closeActiveEditor",     "when": "editorTextFocus" },
    { "key": "cmd+l", "command": "workbench.action.nextEditor"                                       },
    { "key": "cmd+h", "command": "workbench.action.previousEditor"                                   },

    { "key": "cmd+e",       "command": "workbench.action.focusSideBar"                                   },
    { "key": "cmd+e",       "command": "workbench.action.focusActiveEditorGroup", "when": "sideBarFocus" },
    { "key": "cmd+shift+e", "command": "workbench.action.toggleSidebarVisibility"                        },

    { "key": "cmd+shift+1", "command": "workbench.view.explorer"   },
    { "key": "cmd+shift+2", "command": "workbench.view.search"     },
    { "key": "cmd+shift+3", "command": "workbench.view.scm"        },
    { "key": "cmd+shift+4", "command": "workbench.view.debug"      },
    { "key": "cmd+shift+5", "command": "workbench.view.extensions" },
    { "key": "cmd+shift+6", "command": "workbench.view.remote"     },

    { "key": "cmd+n", "command": "workbench.action.terminal.new", "when": "terminalFocus" },
    { "key": "cmd+q", "command": "workbench.action.terminal.kill", "when": "terminalFocus" },
    { "key": "cmd+h", "command": "workbench.action.terminal.focusPrevious", "when": "terminalFocus" },
    { "key": "cmd+l", "command": "workbench.action.terminal.focusNext", "when": "terminalFocus" }
]
